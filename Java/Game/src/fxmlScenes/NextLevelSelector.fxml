<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="638.0" prefWidth="720.0" styleClass="root" stylesheets="@BorderPaneStyle.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="NextLevelSelectorController">
   <children>
      <ImageView fx:id="levelImage" fitHeight="321.0" fitWidth="334.0" layoutX="193.0" layoutY="81.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="next" layoutX="151.0" layoutY="437.0" mnemonicParsing="false" onAction="#handlePlayLevelAction" prefHeight="48.0" prefWidth="444.0" style="-fx-background-color: black;" text="Next" textFill="#eee718">
         <font>
            <Font name="Broadway" size="25.0" />
         </font>
         <effect>
            <Glow level="1.0" />
         </effect>
      </Button>
      <Button fx:id="retry" layoutX="171.0" layoutY="486.0" mnemonicParsing="false" onAction="#handleRetryLevelAction" prefHeight="48.0" prefWidth="410.0" style="-fx-background-color: black;" text="Retry" textFill="#e5da07">
         <font>
            <Font name="Broadway" size="25.0" />
         </font>
         <effect>
            <Glow level="1.0" />
         </effect>
      </Button>
      <Button fx:id="exit" layoutX="164.0" layoutY="532.0" mnemonicParsing="false" onAction="#handleBackAction" prefHeight="54.0" prefWidth="418.0" style="-fx-background-color: black;" text="Exit" textFill="#e3e31c">
         <font>
            <Font name="Broadway" size="25.0" />
         </font>
         <effect>
            <Glow level="1.0" />
         </effect>
      </Button>
      <Label fx:id="score" layoutX="286.0" layoutY="405.0" prefHeight="36.0" prefWidth="289.0" textFill="#eee718">
         <font>
            <Font size="25.0" />
         </font>
         <effect>
            <Glow level="1.0" />
         </effect>
      </Label>
   </children>
</AnchorPane>
